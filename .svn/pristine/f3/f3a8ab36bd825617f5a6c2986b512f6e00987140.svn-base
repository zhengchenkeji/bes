          v-hasPermi="['safetyWarning:alarmWorkOrder:export']"
        >导出</el-button>
      </el-col>
      <right-toolbar :showSearch.sync="showSearch" @queryTable="getList"></right-toolbar>
    </el-row>

    <el-table v-loading="loading" :data="alarmWorkOrderList" @selection-change="handleSelectionChange">
      <el-table-column type="selection" width="55" align="center" />
      <el-table-column label="主键" align="center" prop="id" />
      <el-table-column label="告警策略" align="center" prop="alarmTacticsId" />
      <el-table-column label="告警值" align="center" prop="alarmValue" />
      <el-table-column label="第一次产生时间" align="center" prop="firstTime" width="180">
        <template slot-scope="scope">
          <span>{{ parseTime(scope.row.firstTime, '{y}-{m}-{d}') }}</span>
        </template>
      </el-table-column>
      <el-table-column label="最后一次产生时间" align="center" prop="lastTime" width="180">
        <template slot-scope="scope">
          <span>{{ parseTime(scope.row.lastTime, '{y}-{m}-{d}') }}</span>
        </template>
      </el-table-column>
      <el-table-column label="告警次数" align="center" prop="amount" />
      <el-table-column label="确认状态 0：未确认、1：已确认" align="center" prop="confirmState" />
      <el-table-column label="告警位置" align="center" prop="azwz" />
      <el-table-column label="报警名称" align="center" prop="alarmName" />
      <el-table-column label="计划值" align="center" prop="planVal" />
      <el-table-column label="提示信息 " align="center" prop="promptMsg" />
      <el-table-column label="告警等级 1：一般、2：较大、3：严重" align="center" prop="level" />
      <el-table-column label="所属告警类型 1:电表  2:支路  3:分户 4:分项 5:设备报警" align="center" prop="alarmTypeId" />
      <el-table-column label="所属用户id" align="center" prop="userId" />
      <el-table-column label="处理人编码" align="center" prop="updateCode" />
      <el-table-column label="处理人" align="center" prop="updateName" />
      <el-table-column label="状态(0未处理 1已处理)" align="center" prop="status" />
      <el-table-column label="操作" align="center" class-name="small-padding fixed-width">
        <template slot-scope="scope">
          <el-button
            size="mini"
            type="text"
            icon="el-icon-edit"
            @click="handleUpdate(scope.row)"
            v-hasPermi="['safetyWarning:alarmWorkOrder:edit']"
          >修改</el-button>
          <el-button
            size="mini"
            type="text"
            icon="el-icon-delete"
            @click="handleDelete(scope.row)"
            v-hasPermi="['safetyWarning:alarmWorkOrder:remove']"
          >删除</el-button>
        </template>
      </el-table-column>
    </el-table>

    <pagination
      v-show="total>0"
      :total="total"
      :page.sync="queryParams.pageNum"
      :limit.sync="queryParams.pageSize"
      @pagination="getList"
    />

    <!-- 添加或修改告警工单对话框 -->
    <el-dialog :title="title" :visible.sync="open" width="500px" append-to-body>
      <el-form ref="form" :model="form" :rules="rules" label-width="80px">
        <el-form-item label="告警策略" prop="alarmTacticsId">
          <el-input v-model="form.alarmTacticsId" placeholder="请输入告警策略" />
        </el-form-item>
        <el-form-item label="告警值" prop="alarmValue">
          <el-input v-model="form.alarmValue" placeholder="请输入告警值" />
        </el-form-item>
        <el-form-item label="第一次产生时间" prop="firstTime">
          <el-date-picker clearable
            v-model="form.firstTime"
            type="date"
            value-format="yyyy-MM-dd"
            placeholder="请选择第一次产生时间">
          </el-date-picker>
        </el-form-item>
        <el-form-item label="最后一次产生时间" prop="lastTime">
          <el-date-picker clearable
            v-model="form.lastTime"
            type="date"
            value-format="yyyy-MM-dd"
            placeholder="请选择最后一次产生时间">
          </el-date-picker>
        </el-form-item>
        <el-form-item label="告警次数" prop="amount">
          <el-input v-model="form.amount" placeholder="请输入告警次数" />
        </el-form-item>
        <el-form-item label="确认状态 0：未确认、1：已确认" prop="confirmState">
          <el-input v-model="form.confirmState" placeholder="请输入确认状态 0：未确认、1：已确认" />
        </el-form-item>
        <el-form-item label="告警位置" prop="azwz">
          <el-input v-model="form.azwz" placeholder="请输入告警位置" />
        </el-form-item>
        <el-form-item label="报警名称" prop="alarmName">
          <el-input v-model="form.alarmName" placeholder="请输入报警名称" />
        </el-form-item>
        <el-form-item label="计划值" prop="planVal">
          <el-input v-model="form.planVal" placeholder="请输入计划值" />
        </el-form-item>
        <el-form-item label="提示信息 " prop="promptMsg">
          <el-input v-model="form.promptMsg" placeholder="请输入提示信息 " />
        </el-form-item>
        <el-form-item label="告警等级 1：一般、2：较大、3：严重" prop="level">
          <el-input v-model="form.level" placeholder="请输入告警等级 1：一般、2：较大、3：严重" />
        </el-form-item>
        <el-form-item label="所属告警类型 1:电表  2:支路  3:分户 4:分项 5:设备报警" prop="alarmTypeId">
          <el-input v-model="form.alarmTypeId" placeholder="请输入所属告警类型 1:电表  2:支路  3:分户 4:分项 5:设备报警" />
        </el-form-item>
        <el-form-item label="所属用户id" prop="userId">
          <el-input v-model="form.userId" type="textarea" placeholder="请输入内容" />
        </el-form-item>
        <el-form-item label="处理人编码" prop="updateCode">
          <el-input v-model="form.updateCode" placeholder="请输入处理人编码" />
        </el-form-item>
        <el-form-item label="处理人" prop="updateName">
