// theme-blue
.theme-blue {
  .el-table--border::after,
  .el-table--group::after,
  .el-table::before {
    background-color: $theme-blue-border-color;
  }
  .el-popover {
    border: 1px solid $theme-blue-border-color;
  }
  .el-table td.el-table__cell,
  .el-table th.el-table__cell.is-leaf {
    border: 1px solid $theme-blue-border-color;
  }
  .el-pagination.is-background .btn-next,
  .el-pagination.is-background .btn-prev,
  .el-pagination.is-background .el-pager li {
    background-color: #004a77;
    color: #ffffff;
    border: 1px solid #4f8aaa;
  }
  .el-popover {
    color: white;
  }
  .el-pagination__jump {
    color: white;
  }
  .pagination-container {
    background: $theme-blue-bg-color !important;
  }
  .el-pagination__total {
    color: white;
  }
  .el-table--enable-row-hover .el-table__body tr:hover > td.el-table__cell {
    background-color: #0d6da7;
  }
  .el-table td.el-table__cell,
  .el-table th.el-table__cell.is-leaf {
    background-color: #075e8d;
    color: white;
  }
  .el-table th.el-table__cell {
    background-color: #075e8d;
    color: white;
  }
  // 单选组
  .el-radio-group {
    .el-radio-button {
      .el-radio-button__inner {
        background: $theme-blue-bg-color;
        color: $theme-blue-text-color;
        border: 1px solid $theme-blue-border-color;
      }
    }
    .is-active {
      .el-radio-button__inner {
        background: #0d6da7;
      }
    }
  }
  .el-input-number__increase {
    border-left: 1px solid #4f8aaa;
  }
  .el-input-number__decrease {
    border-right: 1px solid #4f8aaa;
  }
  // 左右增减按钮
  .is-controls-right {
    .el-input-number__decrease {
      border-left: 1px solid $theme-blue-border-color;
    }
    .el-input-number__increase {
      border-bottom: 1px solid $theme-blue-border-color;
    }
  }
  .el-input-number__increase,
  .el-input-number__decrease {
    background: #306691;
    color: white;
  }
  .el-input-group__append {
    background-color: $theme-blue-bg-color;
    border: 1px solid $theme-blue-border-color;
  }
  // type="primary" 按钮
  .el-input-group__append button.el-button {
    background-color: #306691;
    border-color: $theme-blue-border-color;
    color: white;
    border-top: 1px solid $theme-blue-border-color;
    border-bottom: 1px solid $theme-blue-border-color;
  }
  .el-color-picker__trigger {
    border: 1px solid $theme-blue-border-color;
  }
  // 颜色选择器
  .el-color-dropdown {
    background: #004a77;
    border: 1px solid $theme-blue-border-color;
    .el-color-dropdown__btns {
      .el-color-dropdown__btn {
        color: #ffffff;
        background: #306691;
        border-color: #4a88b1;
      }
    }
  }
  .rightPanel-background {
    overflow: hidden;
  }
  // 主题风格设置
  .rightPanel {
    background: #004a77;
    overflow: hidden;
    height: 100%;
    .rightPanel-items {
      .el-divider {
        background-color: $theme-blue-border-color;
      }
      .drawer-title {
        color: white;
      }
      .drawer-item {
        span {
          color: white;
        }
      }
    }
  }
  .el-tabs__nav-wrap::after {
    background-color: #4f8aaa;
  }
  // 文本域
  .el-textarea__inner {
    background-color: $theme-blue-bg-color;
    color: white;
    border: 1px solid #4f8aaa;
  }
  .el-date-picker__header--bordered {
    border-bottom: 1px solid #4f8aaa;
  }
  .el-year-table td .cell {
    color: white;
  }
  .el-year-table .today .cell {
    color: #6da5f1;
  }
  .el-date-picker__header-label {
    color: white;
  }
  .vue-treeselect__control {
    background-color: #004a77;
    border: 1px solid #4f8aaa;
    .vue-treeselect__single-value {
      color: white;
    }
  }
  .vue-treeselect:not(.vue-treeselect--disabled):not(.vue-treeselect--focused)
    .vue-treeselect__control:hover {
    background-color: #004a77;
    border: 1px solid #4f8aaa;
  }
  .el-select-dropdown.is-multiple .el-select-dropdown__item.selected {
    background-color: #0d6da7;
    color: white;
  }
  .el-tabs--border-card > .el-tabs__header .el-tabs__item.is-active {
    border-left: 1px solid $theme-blue-border-color;
    border-right: 1px solid $theme-blue-border-color;
  }
  .el-input__inner {
    background: #004a77;
    border: 1px solid $theme-blue-border-color;
    color: $theme-blue-text-color;
  }
  .icon-body {
    .icon-list div {
      color: white;
    }
  }
  .el-popover {
    background-color: $theme-blue-bg-color;
    border: 1px solid #4f8aaa;
  }
  .vue-treeselect__option--selected {
    background-color: #0d6da7;
  }
  .vue-treeselect--single .vue-treeselect__option--selected:hover {
    background-color: $theme-blue-bg-color;
  }
  .vue-treeselect--open-below .vue-treeselect__menu {
    border: 1px solid $theme-blue-border-color;
  }
  .vue-treeselect__menu-container {
    background-color: $theme-blue-bg-color;
    div {
      background-color: $theme-blue-bg-color;
      color: white;
    }
  }
  .ace-jsoneditor .ace_marker-layer .ace_active-line {
    background-color: #0d6da7;
  }
  div.jsoneditor {
    border: 1px solid $theme-blue-border-color;
  }
  .el-month-table td .cell:hover {
    color: #6da5f1;
  }
  .el-month-table td .cell {
    color: white;
  }
  .ace_line_group .ace_line {
    color: white;
  }
  .ace-jsoneditor .ace_string {
    color: white;
  }
  .ace-jsoneditor .ace_variable {
    color: white;
  }
  .ace-jsoneditor .ace_gutter {
    background-color: $theme-blue-bg-color;
    color: white;
  }
  .ace-jsoneditor .ace_scroller {
    background-color: $theme-blue-bg-color;
  }
  .ace-jsoneditor .ace_gutter-active-line {
    background-color: #0d6da7;
  }
  div.jsoneditor-menu {
    background-color: $theme-blue-bg-color;
  }
  .el-drawer__wrapper {
    .el-drawer__body {
      .el-tabs--border-card > .el-tabs__header {
        background-color: $theme-blue-bg-color;
      }
      .el-tabs--border-card {
        border: 0px;
        background: $theme-blue-bg-color;
        .el-descriptions-item__label.is-bordered-label {
          background: $theme-blue-bg-color;
          color: white;
        }
        .el-descriptions .is-bordered .el-descriptions-item__cell {
          border: 1px solid $theme-blue-border-color;
        }
        td {
          background: $theme-blue-bg-color;
          color: white;
          border: 1px solid $theme-blue-border-color;
        }
      }
    }
  }
  .card-box {
    .el-card__body {
      .el-table {
        thead {
          color: white;
        }
        th.el-table__cell {
          background-color: $theme-blue-bg-color;
        }
        tr {
          background-color: $theme-blue-bg-color;
          th {
            border-bottom: $theme-blue-border-color;
          }
          .el-table th.el-table__cell.is-leaf,
          .el-table td.el-table__cell {
            color: white;
          }
        }
      }
    }
  }
  .el-table th.el-table__cell .card-box {
    .el-card__body {
      tr {
        background-color: $theme-blue-bg-color;
      }
      .el-table th.el-table__cell {
        background-color: $theme-blue-bg-color;
      }
    }
  }
  .el-message-box__wrapper {
    .el-message-box {
      background-color: rgba(0, 74, 119, 0.9);
      border: 1px solid $theme-blue-border-color;
      .el-message-box__message {
        color: white;
      }
      .el-message-box__title {
        span {
          color: white;
        }
      }
    }
  }
  .el-dropdown-menu {
    background-color: $theme-blue-bg-color;
    border: 1px solid $theme-blue-border-color;
    .el-dropdown-menu__item:not(.is-disabled):hover {
      background-color: #2371a1;
    }
    .el-dropdown-menu__item--divided {
      border-top: 1px solid #2371a1;
    }
    .el-dropdown-menu__item {
      color: $theme-blue-text-color;
    }
    .el-dropdown-menu__item--divided:before {
      background-color: $theme-blue-bg-color;
    }
  }
  // 下面下拉框三角
  .el-popper[x-placement^="bottom"] .popper__arrow::after {
    border-bottom-color: #004a77;
  }
  // 上面下拉框三角
  .el-popper[x-placement^="top"] .popper__arrow::after {
    border-top-color: #004a77;
  }
  .el-input__inner:hover {
    border-color: $theme-blue-border-color;
  }
  //抽屉
  .el-drawer__wrapper {
    .el-drawer__container .el-drawer.rtl {
      background: rgba(0, 74, 119, 0.9);
    }
    .el-drawer__header {
      .el-drawer__close-btn,
      span {
        color: white;
      }
    }
    .el-drawer__body {
      .el-input {
        .el-input-group__append {
          background-color: #4a79ba;
          border: 1px solid $theme-blue-border-color;
          color: white;
        }
      }
      .el-tabs--border-card > .el-tabs__header {
        border-bottom: 1px solid $theme-blue-border-color;
        .el-tabs__item.is-active {
          border-right-color: $theme-blue-border-color;
          border-left-color: $theme-blue-border-color;
        }
      }
      .el-tabs__nav-scroll {
        .el-tabs__item {
          color: #d3cccc;
        }
        .is-active {
          // color: #2578e9;
          color: white;
          background-color: #0d6da7;
        }
      }
      .el-tabs__content {
        .input-with-select_title {
          color: white;
        }
      }
    }
    //
  }

  // 弹出窗
  .el-dialog__wrapper {
    background-color: rgba(0, 37, 60, 0.2);

    .el-dialog {
      .el-dialog__headerbtn .el-dialog__close {
        color: white;
      }
      // 模态框背景颜色
      background-color: rgba(19, 104, 150, 1);
      .el-dialog__title {
        color: white;
      }
      .el-dialog__body {
        .el-tabs__content {
          /*代码预览-复制按钮*/
          .el-link.el-link--default {
            color: $theme-blue-text-color;
          }
          .hljs {
            background: rgba(19, 104, 150, 0.8);
            color: white;
            .hljs-keyword {
              color: #6badff;
            }
            .hljs-string {
              color: #3eafff;
            }
            .hljs-title {
              color: #75acff;
            }
          }
        }
        .el-tabs__header {
          .el-tabs__item,
          .is_top {
            color: white;
          }
        }
        .el-tabs__nav-scroll {
          .el-tabs__active-bar {
            background-color: #2e86ff;
          }
        }
        .el-transfer-panel {
          background: rgba(19, 104, 150, 0.8);
          border: 1px solid $theme-blue-border-color;
        }
        .el-transfer-panel .el-transfer-panel__header {
          border-bottom: 1px solid $theme-blue-border-color;
        }
        .el-transfer-panel .el-transfer-panel__header {
          background: rgba(19, 104, 150, 0.8);
        }
        .el-transfer-panel .el-transfer-panel__empty {
          color: white;
        }
        .el-select-dropdown.is-multiple .el-select-dropdown__item.selected {
          background-color: #0d6da7;
        }
        .el-transfer-panel
          .el-transfer-panel__header
          .el-checkbox
          .el-checkbox__label {
          color: white;
        }
        .el-tabs--border-card > .el-tabs__header {
          background-color: rgba(19, 104, 150, 1);
          border-bottom: 1px solid $theme-blue-border-color;
        }
        .el-tabs--border-card > .el-tabs__header .el-tabs__item.is-active {
          color: white;
          background-color: #0d6da7;
        }
        .el-tabs--border-card {
          background: rgba(19, 104, 150, 0.8);
          border: 1px solid $theme-blue-border-color;
        }
        .popup-main {
          background: rgba(19, 104, 150, 0.8);
        }
        .popup-result {
          color: white;
          border: 1px solid $theme-blue-border-color;
          .title {
            background-color: rgba(19, 104, 150, 1);
          }
          table {
            span {
              border: 1px solid $theme-blue-border-color;
            }
            th {
              color: white;
            }
          }
        }
        .ql-snow.ql-toolbar button:hover,
        .ql-snow .ql-toolbar button:hover,
        .ql-snow.ql-toolbar button:focus,
        .ql-snow .ql-toolbar button:focus,
        .ql-snow.ql-toolbar button.ql-active,
        .ql-snow .ql-toolbar button.ql-active,
        .ql-snow.ql-toolbar .ql-picker-label:hover,
        .ql-snow .ql-toolbar .ql-picker-label:hover,
        .ql-snow.ql-toolbar .ql-picker-label.ql-active,
        .ql-snow .ql-toolbar .ql-picker-label.ql-active,
        .ql-snow.ql-toolbar .ql-picker-item:hover,
        .ql-snow .ql-toolbar .ql-picker-item:hover,
        .ql-snow.ql-toolbar .ql-picker-item.ql-selected,
        .ql-snow .ql-toolbar .ql-picker-item.ql-selected {
          color: white;
        }
        .ql-snow .ql-stroke {
          stroke: white;
        }
        .ql-snow .ql-picker {
          color: white;
        }
        .ql-snow .ql-picker-options {
          background-color: $theme-blue-bg-color;
        }
        .ql-snow .ql-fill,
        .ql-snow .ql-stroke.ql-fill {
          fill: white;
        }
        .ql-editor.ql-blank::before {
          color: white;
        }
        .ql-toolbar.ql-snow .ql-picker.ql-expanded .ql-picker-options {
          border: 1px solid $theme-blue-border-color;
        }
        .ql-toolbar.ql-snow .ql-picker.ql-expanded .ql-picker-label {
          border-color: $theme-blue-border-color;
        }
        .ql-snow .ql-picker.ql-expanded .ql-picker-label {
          border: 1px solid white;
        }
        .ql-toolbar.ql-snow {
          border: 1px solid $theme-blue-border-color;
        }
        .ql-container.ql-snow {
          border: 1px solid $theme-blue-border-color;
        }
        // .ql-snow .ql-stroke {
        //   color: white;
        // }
        .el-form-item {
          .el-form-item__content {
            color: white;
            .el-select {
              .el-tag.el-tag--info {
                background-color: #22b2ff;
                border: 1px solid $theme-blue-border-color;
                span {
                  color: white;
                }
              }
            }
            .vue-treeselect__control {
              border: 1px solid $theme-blue-border-color;
            }
            .el-textarea__inner {
              border: 1px solid $theme-blue-border-color;
            }
            .el-textarea__inner:hover {
              border: 1px solid $theme-blue-border-color;
            }
            .tree-border {
              border: 1px solid $theme-blue-border-color;
              background: rgba(19, 104, 150, 0.8) none;
              .el-tree-node:focus > .el-tree-node__content {
                background-color: $theme-blue-bg-color;
              }
              .el-tree-node__content {
                .el-tree-node__label {
                  color: white;
                }
              }
              .el-tree-node__content:hover {
                background-color: #2371a1;
              }
            }
          }
        }

        .el-checkbox__input.is-checked + .el-checkbox__label {
          color: #22b2ff;
        }
        .el-checkbox__input.is-checked .el-checkbox__inner {
          background-color: #22b2ff;
        }
        .el-checkbox {
          color: white;
        }
        .el-form-item__label {
          color: white;
        }
        input:-internal-autofill-selected {
          background-color: #004a77;
        }
        .el-input__inner,
        .el-textarea__inner,
        .vue-treeselect__control {
          background-color: #004a77;
          color: white;
          .vue-treeselect__single-value {
            color: white;
          }
        }
        .el-input__inner {
          border: 1px solid $theme-blue-border-color;
        }
        .vue-treeselect:not(.vue-treeselect--disabled)
          .vue-treeselect__control-arrow-container:hover
          .vue-treeselect__control-arrow {
          color: white;
        }
        .vue-treeselect__menu-container {
          background-color: #004a77;
        }
        .vue-treeselect__menu-item {
          background-color: #004a77;
        }
      }
    }
  }
  .el-radio {
    color: white;
  }
  .el-radio__input.is-checked .el-radio__inner {
    color: #22b2ff;
    background: #22b2ff;
  }
  .el-radio__input.is-checked + .el-radio__label {
    color: #22b2ff;
  }
  .el-select-dropdown.is-multiple .el-select-dropdown__item.selected.hover {
    background-color: #0d6da7;
  }
  .el-select-dropdown {
    background-color: rgba(0, 74, 119, 0.9);
    border: 1px solid #429bcb;
    .el-scrollbar {
      border-radius: 3px;
    }
    .el-scrollbar__wrap::-webkit-scrollbar-track {
      background-color: #2087c2;
    }

    .el-scrollbar__wrap::-webkit-scrollbar {
      width: 12px;
      background-color: #2087c2;
    }

    .el-scrollbar__wrap::-webkit-scrollbar-thumb {
      background-color: #2087c2;
    }
  }
  .el-date-range-picker__content.is-left {
    border-right: 1px solid $theme-blue-border-color;
  }
  .el-dropdown-menu_theme-blue {
    .el-scrollbar__wrap::-webkit-scrollbar-track {
      background-color: $theme-blue-rgba;
    }

    .el-scrollbar__wrap::-webkit-scrollbar {
      width: 12px;
      background-color: $theme-blue-rgba;
    }

    .el-scrollbar__wrap::-webkit-scrollbar-thumb {
      background-color: $theme-blue-rgba;
    }
  }
  // 全局滚动条设置
  .el-submenu {
    position: relative;
  }
  .el-button--primary {
    color: #ffffff;
    background: #306691;
    border-color: #4a88b1;
  }
  .el-button--primary:hover {
    background: #4a88b1;
  }
  .el-button--warning.is-plain {
    color: #ffffff;
    background: #50b590d9;
    border-color: #4a88b1;
  }
  .el-button--warning.is-plain:hover {
    background: #57c59cf2;
  }
  .el-date-table td.today span {
    color: #6da5f1;
  }
  .el-date-table td.in-range div {
    color: $theme-blue-bg-color;
  }
  .el-date-table td.start-date span,
  .el-date-table td.end-date span {
    background-color: $theme-blue-bg-color;
    color: $theme-blue-text-color;
  }
  .el-picker-panel {
    background-color: $theme-blue-bg-color;
    border: 1px solid $theme-blue-border-color;
    .el-picker-panel__icon-btn {
      color: $theme-blue-text-color;
    }
    .el-date-range-picker__content .el-date-range-picker__header div,
    .el-date-table th,
    .el-date-table td {
      color: $theme-blue-text-color;
    }
    .el-date-table th {
      border-bottom: 1px solid $theme-blue-border-color;
    }
  }
  .el-select-dropdown__list {
    background-color: $theme-blue-bg-color;
    // 下拉框选中效果
    .el-select-dropdown__item,
    .el-select-dropdown__item {
      color: white;
    }
    .el-select-dropdown__item.hover,
    .el-select-dropdown__item:hover {
      background-color: #2371a1;
      color: white;
    }
  }
  .el-dropdown-menu
    .el-tabs
    .el-tabs__content
    .el-scrollbar
    .el-scrollbar__view
    .warnItem {
    border-bottom: 1px solid #0570b1;
  }
  #app {
    .el-card {
      background-color: #004a77;
      border: 1px solid $theme-blue-border-color;
      .el-card__header {
        border-bottom: 1px solid $theme-blue-border-color;
        span {
          color: white;
        }
      }
      .list-group-item {
        border-top: 1px solid $theme-blue-border-color;
        border-bottom: 1px solid $theme-blue-border-color;
        color: white;
      }
      .el-tabs__item.is-active {
        color: #4191fe;
      }
      .el-tabs__item:hover {
        color: #4191fe;
      }
      .el-tabs__item {
        color: white;
      }
      .user-info-head {
        .img-lg {
          color: white;
        }
      }
    }
    // #004a77
    .el-scrollbar__wrap::-webkit-scrollbar-track {
      background-color: #2087c2;
    }

    .el-scrollbar__wrap::-webkit-scrollbar {
      width: 12px;
      background-color: #f5f5f5;
    }

    .el-scrollbar__wrap::-webkit-scrollbar-thumb {
      background-color: #2087c2;
    }

    .app-main {
      .pagination-container .el-pagination.is-background .el-pager .active {
        color: #1987c2;
      }
      .el-tree-node:focus > .el-tree-node__content {
        background-color: $theme-blue-bg-color;
      }
    }
    .el-menu--collapse .el-submenu {
      position: relative;
    }
    .el-breadcrumb span,
    .el-breadcrumb__inner a,
    .el-form-item__label {
      color: $theme-blue-text-color;
    }
    .el-input__inner {
      background-color: $theme-blue-input-color;
      border: 1px solid $theme-blue-border-color;
      color: $theme-blue-text-color;
    }
    .el-date-editor .el-range-input {
      background-color: $theme-blue-input-color;
      color: $theme-blue-text-color;
    }
    .el-tree {
      background-color: $theme-blue-bg-color;
      .el-tree-node__label {
        color: white;
      }
      .el-tree-node__content:hover {
        background-color: #136896;
      }
      .el-tree-node {
        .is-current {
          background-color: #075e8d;
        }
      }
      .el-tree-node:focus > .el-tree-node__content {
        background-color: #075e8d;
      }
      .is-current {
        .el-tree-node__children {
          background-color: $theme-blue-bg-color;
        }
      }
    }
    .el-table {
      background-color: $theme-blue-bg-color;
      .el-table__header-wrapper th {
        background-color: #1987c2;
        border-bottom: 0px;
        color: white;
      }
      .el-table__row {
        td {
          background-color: #075e8d;
          // iot设备页在线状态样式
          .dot {
            display: flex;
            align-items: center;
            justify-content: center;
            .point {
              margin: 0 10px;
              width: 8px;
              height: 8px;
              display: block;
              border-radius: 35px;
              -webkit-border-radius: 35px;
              -moz-border-radius: 35px;
              -ms-border-radius: 35px;
              -o-border-radius: 35px;
            }
            .greenDot {
              background-color: #0bf10b;
            }
            .redDot {
              background-color: red;
            }
          }
        }
        .el-tooltip {
          span {
            color: #4cbdff;
          }
        }
      }
      .el-table__row.el-table__row--striped td {
        background-color: #136896 !important;
      }
    }
    .el-switch.is-checked .el-switch__core {
      background-color: #39b1f1;
    }
    .el-table--enable-row-hover .el-table__body tr:hover > td.el-table__cell {
      background-color: #0d6da7 !important;
    }
    .el-table td.el-table__cell {
      border: unset;
    }
    .el-table::before {
      height: 0px;
    }
    th.el-table__cell > {
      color: $theme-blue-text-color;
    }
    td {
      div {
        color: $theme-blue-text-color;
      }
      .el-button {
        i,
        span {
          color: #4cbdff;
        }
      }
      .is-disabled {
        i {
          color: #999;
        }
        span {
          color: #999;
        }
      }
    }
    .el-table--striped
      .el-table__body
      tr.el-table__row--striped
      td.el-table__cell {
      background-color: #075e8d;
    }
    .pagination-container {
      background-color: $theme-blue-bg-color;
      .el-pagination__total {
        color: $theme-blue-text-color;
      }
      .el-pagination.is-background .btn-prev,
      .el-pagination.is-background .btn-next,
      .el-pagination.is-background .el-pager li {
        background-color: $theme-blue-bg-color;
        color: $theme-blue-text-color;
        border: 1px solid $theme-blue-border-color;
      }
      .el-pagination__jump {
        color: $theme-blue-text-color;
      }
    }
    .el-date-editor .el-range-separator {
      color: $theme-blue-text-color;
    }
    .app-container {
      // loading背景色
      .el-loading-mask {
        background-color: unset;
      }
      .top-right-btn {
        .el-button {
          background-color: $theme-blue-bg-color;
          border: 1px solid $theme-blue-border-color;
          i {
            color: white;
          }
        }
      }
    }
  }
  /*选中树高亮保持*/
  .el-tree--highlight-current .el-tree-node.is-current > .el-tree-node__content {
    background-color: #075e8d;
  }
}
// theme-light
.theme-light {
  .sidebar-container {
    .el-scrollbar {
      .scrollbar-wrapper.el-scrollbar__wrap {
        margin: 0px !important;
        //overflow: hidden;
        overflow-y: auto;
      }
    }
  }
  .el-submenu__title:hover {
    border-radius: 8px;
    -webkit-border-radius: 8px;
    -moz-border-radius: 8px;
    -ms-border-radius: 8px;
    -o-border-radius: 8px;
  }
  #app {
    // 没有二级菜单情况下选中
    .is_left {
      .router-link-active {
        .is-active {
          background-color: #1d58a9 !important;
          span {
            color: white;
          }
          .svg-icon {
            color: white;
          }
        }
      }
    }
    .is_left {
      .el-menu {
        .nest-menu {
          a {
            .is-active {
              background-color: $theme-dark-bg-color !important;
              color: $theme-light-bg-color !important;
              width: 200px;
              border-radius: 8px;
              -webkit-border-radius: 8px;
              -moz-border-radius: 8px;
              -ms-border-radius: 8px;
              -o-border-radius: 8px;
            }
          }
        }
        .menu-item {
          .el-submenu__title:hover {
            width: unset !important;
          }
          .is-active {
            width: unset;
          }
        }
      }
    }
  }
  #dropdown-menu-3689 {
    background-color: rgba(0, 74, 119, 0.8) !important;
  }
}
.theme-dark {
  .menu-item {
    // background-color: rgba(0, 74, 119, 0.8);
    .el-menu {
      background-color: rgba(0, 74, 119, 0.8);
      .nest-menu {
        a {
          background-color: rgba(0, 74, 119, 0.8);
        }
      }
    }
  }
  .sidebar-container {
    .el-scrollbar {
      .scrollbar-wrapper.el-scrollbar__wrap {
        margin: 0px !important;
        //overflow: hidden;
        overflow-y: auto;
      }
    }
  }
  .el-scrollbar__view {
    .topSubmenu .el-submenu__title:hover {
      width: unset !important;
    }
    .is_left .el-submenu__title:hover {
      //width: 200px;
      border-radius: 8px;
      -webkit-border-radius: 8px;
      -moz-border-radius: 8px;
      -ms-border-radius: 8px;
      -o-border-radius: 8px;
    }
  }
  .index_container {
    background-color: $theme-dark-bg-color;
  }
  .el-select-dropdown {
    .el-popper[x-placement^="bottom"] .popper__arrow::after {
      border-bottom-color: white;
    }
  }
  .onlyPop {
    .popper__arrow::after {
      border-bottom-color: #1d58a9e6;
    }
  }
  #app {
    .sidebar-container .theme-dark .el-submenu .is-active {
      color: $theme-dark-bg-color !important;
    }
    .sidebar-container .el-menu-item#left_nav.is-active {
      background-color: white !important;
      color: $theme-dark-bg-color !important;
      border-radius: 8px;
      -webkit-border-radius: 8px;
      -moz-border-radius: 8px;
      -ms-border-radius: 8px;
      -o-border-radius: 8px;
      //width: 200px;
    }
  }
}

// 处理浮动header
.theme-blue {
  .fixed-header {
    background-color: white;
    width: 100% !important;
  }
}

.clear {
  clear: both;
}
