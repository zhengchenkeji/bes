import request from '@/utils/request'

//查询所有园区
export function getAllPark() {
  return request({
    url: '/energyAnalysis/householdContemporaneous/getAllPark',
    method: 'get',
  })
}
//查询能源类型
export function getEnergyType() {
  return request({
    url: '/energyAnalysis/householdContemporaneous/getEnergyType',
    method: 'get',
  })
}
//查询能耗趋势
export function queryData(param) {
  return request({
    url: '/energyAnalysis/householdContemporaneous/queryData',
    params: param,
    method: 'get',
  })
}
