import request from '@/utils/request'

/******************************查询模块点类型定义列表***************************/
export function listModulePoint(query) {
  return request({
    url: '/basicData/modulePoint/list',
    method: 'get',
    params: query
  })
}

/******************************查询模块点类型定义详细***************************/
export function getModulePoint(id) {
  return request({
    url: '/basicData/modulePoint/' + id,
    method: 'get'
  })
}

/******************************新增模块点类型定义***************************/
export function addModulePoint(data) {
  return request({
    url: '/basicData/modulePoint',
    method: 'post',
    data: data
  })
}

/******************************修改模块点类型定义***************************/
export function updateModulePoint(data) {
  return request({
    url: '/basicData/modulePoint',
    method: 'put',
    data: data
  })
}

/******************************删除模块点类型定义***************************/
export function delModulePoint(id) {
  return request({
    url: '/basicData/modulePoint/' + id,
    method: 'delete'
  })
}

/******************************导出模块点类型定义***************************/
export function exportModulePoint(query) {
  return request({
    url: '/basicData/modulePoint/export',
    method: 'get',
    params: query
  })
}

