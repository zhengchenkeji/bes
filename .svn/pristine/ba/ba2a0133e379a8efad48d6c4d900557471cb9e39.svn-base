import request from '@/utils/request'

// 订阅
export function subscribe(query) {
  return request({
    url: '/basedatamanage/pubsubmanage/subscribe',
    method: 'get',
    params: query
  })
}

// 取消订阅
export function unsubscribe(query) {
  return request({
    url: '/basedatamanage/pubsubmanage/unsubscribe',
    method: 'get',
    params: query
  })
}
