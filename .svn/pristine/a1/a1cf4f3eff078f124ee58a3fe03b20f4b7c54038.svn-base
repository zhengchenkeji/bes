//选则组件时递归遍历树 是灯光则开启禁用，不是则关闭禁用
export function forEachChildren(data, isLight) {
  data.forEach((item, i) => {
    if (item.children != null && item.children != undefined && item.children.length > 0) {
      // 调用递归函数
      forEachChildren(item.children,isLight)
    } else {
      if(isLight){
        //只有读写类型且只有一个寄存器的功能可以选择
        if (item.readType == '1' && item.haveParams == '0') {
          item['disabled'] = false;
        } else {
          //禁用复选框
          item['disabled'] = true;
        }
      }else{
        item['disabled'] = false;
      }

    }
  })
  return data;
}

