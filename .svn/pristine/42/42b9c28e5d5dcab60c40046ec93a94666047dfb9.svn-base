import request from '@/utils/request'

/******************************查询模块类型定义列表***************************/
export function listModuleType(query) {
  return request({
    url: '/basicData/moduleType/list',
    method: 'get',
    params: query
  })
}

/******************************查询模块类型定义详细***************************/
export function getModuleType(id) {
  return request({
    url: '/basicData/moduleType/' + id,
    method: 'get'
  })
}

/******************************新增模块类型定义***************************/
export function addModuleType(data) {
  return request({
    url: '/basicData/moduleType',
    method: 'post',
    data: data
  })
}

/******************************修改模块类型定义***************************/
export function updateModuleType(data) {
  return request({
    url: '/basicData/moduleType',
    method: 'put',
    data: data
  })
}

/******************************删除模块类型定义***************************/
export function delModuleType(id) {
  return request({
    url: '/basicData/moduleType/' + id,
    method: 'delete'
  })
}

/******************************导出模块类型定义***************************/
export function exportModuleType(query) {
  return request({
    url: '/basicData/moduleType/export',
    method: 'get',
    params: query
  })
}

/******************************查询点类型列表***************************/
export function listPoint(query) {
  return request({
    url: '/basicData/moduleType/listPoint',
    method: 'get',
    params: query
  })
}

