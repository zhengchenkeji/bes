import request from '@/utils/request'

//查询所有园区
export function getAllPark() {
  return request({
    url: '/energyAnalysis/survey/getAllPark',
    method: 'get',
  })
}
//查询能源类型
export function getEnergyType() {
  return request({
    url: '/energyAnalysis/survey/getEnergyType',
    method: 'get',
  })
}
/**加载数据*/
//查询能源类型
export function getReportFormList(query) {
  return request({
    url: '/energyAnalysis/reportForm/list',
    method: 'get',
    params: query

  })
}
