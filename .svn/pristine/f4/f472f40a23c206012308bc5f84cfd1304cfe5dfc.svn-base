package com.ruoyi.efounder.JEnterprise.domain.designer;

import com.ruoyi.common.core.domain.BaseEntity;

/**
 * @Author: wanghongjie
 * @Description:设计器区域对象
 * @Date: Created in 8:43 2022/10/20
 * @Modified By:
 */
public class DesignerArea extends BaseEntity {

    /**
     *主键id
     */
    private Long id;

    /**
     *区域名称
     */
    private String name;

    /**
     *备注
     */
    private String remark;

    /**
     *父节点编号
     */
    private Long parentId;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public Long getParentId() {
        return parentId;
    }

    public void setParentId(Long parentId) {
        this.parentId = parentId;
    }

    @Override
    public String getRemark() {
        return remark;
    }

    @Override
    public void setRemark(String remark) {
        this.remark = remark;
    }
}
