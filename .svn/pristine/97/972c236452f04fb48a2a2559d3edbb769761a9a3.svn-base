            <if test="buildGroupId != null "> and build_group_id = #{buildGroupId}</if>
            <if test="buildName != null  and buildName != ''"> and build_name like concat('%', #{buildName}, '%')</if>
            <if test="parkId != null  and parkId != ''"> and park_id = #{parkId}</if>
        </where>
