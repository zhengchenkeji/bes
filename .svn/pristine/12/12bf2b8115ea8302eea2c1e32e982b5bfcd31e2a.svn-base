package com.zc.connect.client.core;

/**
 * 消息接收
 */
public interface DataReceiveCallback
{
    /**
     * 接收消息
     * @param data 数据
     * @param ip ip 地址
     * @param port 端口
     */
    void receiveData(String data, String ip, Integer port);

    /**
     * 在线状态信息
     * @param state 在线状态 true 在线 false 离线
     * @param ip ip地址
     * @param port 端口号
     */
    void onlineStateInfo(Boolean state, String ip, Integer port);
}
