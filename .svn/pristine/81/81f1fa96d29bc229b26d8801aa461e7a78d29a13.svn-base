package com.ruoyi.energyCollection.collMethod.mapper;

import com.ruoyi.energyCollection.collMethod.domain.CollMethod;
import org.apache.ibatis.annotations.Param;

import java.util.List;
import java.util.Map;

/**
 * 采集方案Mapper接口
 * 
 * @author ruoyi
 * @date 2022-09-08
 */
public interface CollMethodMapper 
{
    /**
     * 查询采集方案
     * 
     * @param code 采集方案主键
     * @return 采集方案
     */
    public CollMethod selectCollMethodByCode(@Param(value = "code")String code );

    List<CollMethod> selectCollMethodCheck(CollMethod collMethod);

    /**
     * 查询采集方案列表
     * 
     * @param collMethod 采集方案
     * @return 采集方案集合
     */
    List<CollMethod> selectCollMethodList(CollMethod collMethod);

    /**
     * 新增采集方案
     * 
     * @param collMethod 采集方案
     * @return 结果
     */
    int insertCollMethod(CollMethod collMethod);

    /**
     * 修改采集方案
     * 
     * @param collMethod 采集方案
     * @return 结果
     */
    int updateCollMethod(CollMethod collMethod);

    /**
     * 删除采集方案
     * 
     * @param code 采集方案主键
     * @return 结果
     */
    int deleteCollMethodByCode(String code);

    /**
     * 批量删除采集方案
     * 
     * @param codes 需要删除的数据主键集合
     * @return 结果
     */
    boolean deleteCollMethodByCodes(String[] codes);

    /**
     * 查询该园区下能耗类型
     */
    List<Map<String,String>> findPark_EnergyType(@Param(value = "fyqbh")String fyqbh,@Param(value = "group")String group);
}
