package com.zc.relay.dto;

import com.zc.constant.IotConstants.OnlineState;

/**
 * 设备状态数据
 * @author Athena-xiepufeng
 */
public class UplinkDeviceStateData {

    /**
     * 设备编号
     */
    private String deviceCode;

    /**
     * 在线状态
     */
    private Integer state;

    public String getDeviceCode() {
        return deviceCode;
    }

    public void setDeviceCode(String deviceCode) {
        this.deviceCode = deviceCode;
    }

    public Integer getState() {
        return state;
    }

    public void setState(Integer state) {
        if (OnlineState.ON_LINE.equals(state) || OnlineState.OFF_LINE.equals(state))
        {
            this.state = state;
        }
    }

    /**
     * 设置成在线
     */
    public void setOnlineState() {
        this.state = OnlineState.ON_LINE;
    }

    /**
     * 设置成离线
     */
    public void setOfflineState() {
        this.state = OnlineState.OFF_LINE;
    }

}
