package com.ruoyi.safetyWarning.alarmNotifier.domain;

import com.ruoyi.common.annotation.Excels;
import org.apache.commons.lang3.builder.ToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;
import com.ruoyi.common.annotation.Excel;
import com.ruoyi.common.core.domain.BaseEntity;

import javax.validation.constraints.Email;
import javax.validation.constraints.NotBlank;

/**
 * 告警接收组对象 alarm_notifier
 *
 * @author sunshangeng
 * @date 2022-09-15
 */
public class AlarmNotifier extends BaseEntity
{
    private static final long serialVersionUID = 1L;

    /** 主键 */
    private Long id;

    /** 姓名 */
    @Excel(name = "姓名")
    @NotBlank(message = "姓名不能为空")
    private String name;

    /** 家庭地址 */
    @Excel(name = "家庭地址")
    @NotBlank(message = "家庭地址不能为空")

    private String homeAddress;

    /** 联系电话 */
    @Excel(name = "联系电话")
    @NotBlank(message = "联系电话不能为空")

    private String tel;

    /** 邮箱 */
    @Excel(name = "邮箱")
    @NotBlank(message = "邮箱不能为空")
    @Email(message = "请输入正确的邮箱地址")
    private String email;

    /** 公司名称 */
    @Excel(name = "公司名称")
    @NotBlank(message = "公司名称不能为空")
    private String company;

    /** 公司联系电话 */
    @Excel(name = "公司联系电话")
    @NotBlank(message = "公司联系电话不能为空")

    private String companyTel;

    /** 岗位 */
    @NotBlank(message = "岗位不能为空")
    private String post;

    /** 所属组织id */
    @NotBlank(message = "组织不能为空")

    private Long groupId;


    @Excel(name = "部门名称")
    private String deptname;

    /**增加告警策略id 用于查询时传参*/
    private String tacticsId;

    /** 增加是否 吃否绑定关联关系参数 用于查询 1 :关联 0：未关联*/
    private Integer  islink;


    /** 告警策略和告警接收组 关联id*/
    private  Long  linkid;



    public void setId(Long id)
    {
        this.id = id;
    }

    public Long getId()
    {
        return id;
    }
    public void setName(String name)
    {
        this.name = name;
    }

    public String getName()
    {
        return name;
    }
    public void setHomeAddress(String homeAddress)
    {
        this.homeAddress = homeAddress;
    }

    public String getHomeAddress()
    {
        return homeAddress;
    }
    public void setTel(String tel)
    {
        this.tel = tel;
    }

    public String getTel()
    {
        return tel;
    }
    public void setEmail(String email)
    {
        this.email = email;
    }

    public String getEmail()
    {
        return email;
    }
    public void setCompany(String company)
    {
        this.company = company;
    }

    public String getCompany()
    {
        return company;
    }
    public void setCompanyTel(String companyTel)
    {
        this.companyTel = companyTel;
    }

    public String getCompanyTel()
    {
        return companyTel;
    }
    public void setPost(String post)
    {
        this.post = post;
    }

    public String getPost()
    {
        return post;
    }
    public void setGroupId(Long groupId)
    {
        this.groupId = groupId;
    }

    public Long getGroupId()
    {
        return groupId;
    }


    public String getDeptname() {
        return deptname;
    }

    public void setDeptname(String deptname) {
        this.deptname = deptname;
    }

    public String getTacticsId() {
        return tacticsId;
    }

    public void setTacticsId(String tacticsId) {
        this.tacticsId = tacticsId;
    }


    public Integer getIslink() {
        return islink;
    }

    public void setIslink(Integer islink) {
        this.islink = islink;
    }


    public Long getLinkid() {
        return linkid;
    }

    public void setLinkid(Long linkid) {
        this.linkid = linkid;
    }
    @Override
    public String toString() {
        return new ToStringBuilder(this,ToStringStyle.MULTI_LINE_STYLE)
            .append("id", getId())
            .append("name", getName())
            .append("homeAddress", getHomeAddress())
            .append("tel", getTel())
            .append("email", getEmail())
            .append("company", getCompany())
            .append("companyTel", getCompanyTel())
            .append("post", getPost())
            .append("groupId", getGroupId())
            .append("createTime", getCreateTime())
            .append("updateTime", getUpdateTime())
            .toString();
    }
}
