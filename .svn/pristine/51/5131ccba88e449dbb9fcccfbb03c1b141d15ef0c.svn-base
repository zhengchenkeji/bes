package com.ruoyi.energyCollection.collMethod.service;

import com.ruoyi.common.core.domain.AjaxResult;
import com.ruoyi.energyCollection.collMethod.domain.CollMethod;

import java.util.List;

/**
 * 采集方案Service接口
 *
 * @author ruoyi
 * @date 2022-09-08
 */
public interface CollMethodService
{
    /**
     * 查询采集方案
     *
     * @param code 采集方案主键
     * @return 采集方案
     */
    public CollMethod selectCollMethodByCode(String code);

    /**
     * 查询采集方案列表
     *
     * @param collMethod 采集方案
     * @return 采集方案集合
     */
    List<CollMethod> selectCollMethodList(CollMethod collMethod);

    /**
     * 新增采集方案
     *
     * @param collMethod 采集方案
     * @return 结果
     */
    AjaxResult insertCollMethod(CollMethod collMethod);

    /**
     * 修改采集方案
     *
     * @param collMethod 采集方案
     * @return 结果
     */
    AjaxResult updateCollMethod(CollMethod collMethod);

    /**
     * 批量删除采集方案
     *
     * @param codes 需要删除的采集方案主键集合
     * @return 结果
     */
    AjaxResult deleteCollMethodByCodes(String[] codes);

    /**
     * 删除采集方案信息
     *
     * @param code 采集方案主键
     * @return 结果
     */
    int deleteCollMethodByCode(String code);
}
