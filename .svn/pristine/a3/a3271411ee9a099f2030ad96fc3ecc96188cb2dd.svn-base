package com.zc.relay.dto;

import java.util.List;

/**
 * 设备参数信息
 * @author Athena-xiepufeng
 */
public class DownDeviceParam
{
    /**
     * 设备id
     */
    private Long deviceId;

    /**
     * 设备编号
     */
    private String deviceCode;

    /**
     * 设备类型
     * 1 - 直连设备
     * 2 - 网关子设备
     * 3 - 网关设备
     */
    private Integer deviceType;

    /**
     * 网关子设备
     */
    private List<DownDeviceParam> downChildrenDeviceParam;

    /**
     * 设备功能
     */
    private List<DownFunctionParam> downFunctionParams;

    public Long getDeviceId() {
        return deviceId;
    }

    public void setDeviceId(Long deviceId) {
        this.deviceId = deviceId;
    }

    public String getDeviceCode() {
        return deviceCode;
    }

    public void setDeviceCode(String deviceCode) {
        this.deviceCode = deviceCode;
    }

    public Integer getDeviceType() {
        return deviceType;
    }

    public void setDeviceType(Integer deviceType) {
        this.deviceType = deviceType;
    }

    public List<DownDeviceParam> getDownChildrenDeviceParam() {
        return downChildrenDeviceParam;
    }

    public void setDownChildrenDeviceParam(List<DownDeviceParam> downChildrenDeviceParam) {
        this.downChildrenDeviceParam = downChildrenDeviceParam;
    }

    public List<DownFunctionParam> getDownFunctionParams() {
        return downFunctionParams;
    }

    public void setDownFunctionParams(List<DownFunctionParam> downFunctionParams) {
        this.downFunctionParams = downFunctionParams;
    }
}
