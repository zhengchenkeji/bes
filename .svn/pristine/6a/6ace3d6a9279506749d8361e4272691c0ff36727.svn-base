<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ruoyi.efounder.JEnterprise.mapper.HouseholdDataMapper" >


	<!-- 更新分户统计数据 -->
    <update id="updateHouseholdData" parameterType="HouseholdData" >
        update athena_bes_household_data
        <set >
            <if test="dataValue != null" >
				data_value = data_value + #{dataValue},
            </if>

			update_time = current_timestamp(),
        </set>
        where household_id = #{householdId}
        and collect_time = #{collectTime}
        and date_type = #{dateType}
    </update>

	<!-- 保存分户统计数据 -->
	<insert id="saveHouseholdData" parameterType="HouseholdData">
		insert into athena_bes_household_data
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="id != null">
				id,
			</if>
			<if test="householdId != null">
				household_id,
			</if>
			<if test="collectTime != null">
				collect_time,
			</if>
			<if test="dataValue != null">
				data_value,
			</if>
			<if test="dateType != null">
				date_type,
			</if>
			<if test="parkId != null">
				park_id,
			</if>
			create_time,
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="id != null">
				#{id},
			</if>
			<if test="householdId != null">
				#{householdId},
			</if>
			<if test="collectTime != null">
				#{collectTime},
			</if>
			<if test="dataValue != null">
				#{dataValue},
			</if>
			<if test="dateType != null">
				#{dateType},
			</if>
			<if test="parkId != null">
				#{parkId},
			</if>
			current_timestamp(),

		</trim>
	</insert>

</mapper>